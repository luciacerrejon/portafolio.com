    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBPKa2eyh-arLV9YwzYXOVkKk1qtpD4I2g"></script>
    <script>
        var map;

        function initMap() {
            map = new google.maps.Map(document.getElementById("map"), {
                center: new google.maps.LatLng(37.318454202156474, -4.723963592735405),
                zoom: 7,
                mapTypeControl: false,
                zoomControl: true,
                scrollwheel: false,
                mapTypeControlOptions: {
                    style: google.maps.MapTypeControlStyle.DROPDOWN_MENU,
                    mapTypeIds: [google.maps.MapTypeId.ROADMAP]
                }
            });

            // var imagen_marker = '../img/airport.svg';
            var marker = new google.maps.Marker({
                position: {
                    lat: 37.378608,
                    lng: -5.983328
                },
                map: map,
                title: "Green Slope S.L.",
                animation: google.maps.Animation.DROP,
                //   icon: imagen_marker,
            });
        }

        initMap();
    </script>

